{
  "name": "@streak-glue/cli",
  "version": "0.1.50",
  "license": "MIT",
  "exports": {
    ".": "./glue.ts"
  },
  "tasks": {
    "cli": "deno run --allow-all glue.ts",
    "cli-debug": "deno run --inspect-brk --allow-all glue.ts dev samples/myExampleWebhook.ts --no-debug",
    "lint": "deno lint --watch",
    "test": "deno test -P --permit-no-files --watch",
    "check": "deno check"
  },
  "test": {
    "permissions": {
      "read": ["./lib/__snapshots__", "./tests/resources"],
      "write": ["./lib/__snapshots__"],
      "env": true
    }
  },
  "unstable": ["kv", "temporal"],
  "imports": {
    "@cliffy/command": "jsr:@cliffy/command@1.0.0-rc.7",
    "@cliffy/prompt": "jsr:@cliffy/prompt@1.0.0-rc.7",
    "@cliffy/table": "jsr:@cliffy/table@1.0.0-rc.7",
    "@cliffy/keypress": "jsr:@cliffy/keypress@1.0.0-rc.7",
    "@opensrc/deno-open": "jsr:@opensrc/deno-open@^1.0.0",
    "@std/assert": "jsr:@std/assert@1",
    "@std/async": "jsr:@std/async@^1.0.9",
    "@std/collections": "jsr:@std/collections@^1.0.10",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.3",
    "@std/encoding": "jsr:@std/encoding@^1.0.5",
    "@std/fs": "jsr:@std/fs@^1.0.6",
    "@std/net": "jsr:@std/net@^1.0.4",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/streams": "jsr:@std/streams@^1.0.9",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@streak-glue/runtime": "jsr:@streak-glue/runtime@^0.2.4",
    "hono": "npm:hono@^4.7.5",
    "ink": "npm:ink@^5.1.1",
    "ink-spinner": "npm:ink-spinner@^5.0.0",
    "it-pushable": "npm:it-pushable@^3.2.3",
    "react": "npm:react@^18.3.1",
    "typescript": "npm:typescript@^5.9.3",
    "zod": "npm:zod@^3.24.0",
    "@std/fmt": "jsr:@std/fmt@^1.0.3",
    "@std/cli": "jsr:@std/cli@^1.0.6"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "npm:@types/react@^18"
  },
  "fmt": {
    "lineWidth": 160
  },
  "lint": {
    "rules": {
      "exclude": ["no-unversioned-import", "no-import-prefix"]
    }
  }
}
