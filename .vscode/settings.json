    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#95d4d0",
    "statusBar.background": "#71c5c0",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#4db6b0",
    "statusBarItem.remoteBackground": "#71c5c0",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#71c5c0",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#71c5c099",
